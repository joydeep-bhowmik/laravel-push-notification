{
  "name": "joydeep-bhowmik/laravel-push-notification",
  "description": "A Laravel package for integrating Firebase Cloud Messaging (FCM) to handle push notifications.",
  "type": "library",
  "version": "v1.0.0",
  "license": "MIT",
  "keywords": [
    "laravel",
    "fireabse push notification",
    "push notification"
  ],
  "require": {
    "laravel/framework": "^11.9",
    "kreait/firebase-php": "^6.0",
    "livewire/volt": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "JoydeepBhowmik\\LaravelPushNotification\\": "src/",
      "JoydeepBhowmik\\LaravelPushNotification\\Tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "joydeep-bhowmik",
      "email": "bhowmikj9432@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require-dev": {
    "orchestra/testbench": "^9.4",
    "pestphp/pest": "^3.1",
    "pestphp/pest-plugin-laravel": "^3.0"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
